CREATE TABLE Inventory(
    item_id INTEGER PRIMARY KEY AUTOINCREMENT,
    item_name CHAR(50),
    item_cost DOUBLE,
    item_count INTEGER,
    item_recieved INTEGER,
    item_sold INTEGER,
    item_onSale BOOL,
    item_salePrice DOUBLE,
    item_taxable BOOL
    );

CREATE TABLE Transaction(
    trans_id INTEGER PRIMARY KEY AUTOINCREMENT,
    trans_date INTEGER,
    trans_total DOUBLE, 
    );

